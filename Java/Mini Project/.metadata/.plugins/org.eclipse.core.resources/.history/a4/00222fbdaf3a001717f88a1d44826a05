package com.mdy.mybbs.presenter;

import java.util.Scanner;

public class BbsPresenter {
	
	boolean runFlag = true;
	
	public void start(){
		Scanner scanner = new Scanner(System.in);
		
		while(runFlag){
			String command = scanner.nextLine();
		}
		
	}
	
	public void end(){
		runFlag = false;
	}
}
