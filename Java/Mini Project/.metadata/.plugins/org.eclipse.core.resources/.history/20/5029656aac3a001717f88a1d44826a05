package com.mdy.mybbs.view;

public class BbsDetail {
	
	public void showDetail(Bbs bbs){
		System.out.println(""+bbs.getId()+" | ");
		System.out.println(""+bbs.getTitle()+" | ");
		System.out.println(""+bbs.getAuthor()+" | ");
		System.out.println(""+bbs.getDate()+" | ");
		System.out.println(""+bbs.getView()+"");
		
	}

}
