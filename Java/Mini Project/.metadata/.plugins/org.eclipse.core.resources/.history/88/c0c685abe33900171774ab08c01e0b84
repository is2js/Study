package com.mdy.java.extend.obj;

public abstract class Fix {
	// 추상 클래스의 특징
	// 1. 인터페이스와 동일하다.
	// 2. 인터페이스 + 코드
	
	public void process(){
		disassemble();
		for(int i=0 ; i<3 ; i++){
			replacePart(); // 항상 세번씩 교체
		}
		clean();
		assemble();
	}
	
	public abstract void disassemble();
	
	public abstract void replacePart();
	
	public abstract void clean();
	
	public abstract void assemble();
}
